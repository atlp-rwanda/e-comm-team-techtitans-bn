{
  "name": "e-comm-team-techtitans-bn",
  "version": "1.0.0",
  "description": "<!-- TABLE OF CONTENTS -->",
  "main": "lib/server.js",
  "scripts": {
    "start": "node lib/server.js",
    "dev": "npm run build && nodemon lib/server.js",
    "build": "babel src --out-dir docs --out-dir lib",
    "lint:fix": "eslint src --fix --cache",
    "pretest": "node tests/preTest.js",
    "test": "jest --detectOpenHandles",
    "coverage": "jest --collect-coverage",
    "migration": "npm run migrate:undo && npm run migrate",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo:all",
    "migrate:dev": "npm migrate:dev:undo && npm migrate:dev:all",
    "migrate:dev:all": "npx sequelize-cli db:migrate",
    "migrate:dev:undo": "npx sequelize-cli db:migrate:undo:all",
    "seed:dev": "npm run seed:dev:undo && npm run seed:dev:do",
    "seed:dev:do": "npx sequelize-cli db:seed:all",
    "seed:dev:undo": "npx sequelize-cli db:seed:undo:all",
    "seed:test": "npm seed:test:undo && npm seed:test:do",
    "seed:test:do": "npx sequelize-cli db:seed:all",
    "seed:test:undo": "npx sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
